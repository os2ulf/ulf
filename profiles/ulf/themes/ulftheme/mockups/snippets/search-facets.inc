    <fieldset class="search-facets--group">
      <legend class="search-facets--header">Vis kun</legend>
      <div class="search-facets--item">
        <input type="checkbox" id="special_1" class="search-facets--input">
        <label href="#" for="special_1" class="search-facets--link">Gratis forløb</label>
      </div>
    </fieldset>
    <fieldset class="search-facets--group">
      <legend class="search-facets--header">Type</legend>
      <div class="search-facets--item">
        <input type="radio" name="type" id="type_1" class="search-facets--input">
        <label href="#" for="type_1" class="search-facets--link">Dagtilbud</label>
      </div>
      <div class="search-facets--item">
        <input type="radio" name="type" id="type_2" class="search-facets--input">
        <label href="#" for="type_2" class="search-facets--link">Grundskole</label>
